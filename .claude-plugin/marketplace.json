{
  "$schema": "https://anthropic.com/claude-code/marketplace.schema.json",
  "name": "showichiro-claude-plugins",
  "version": "1.2.0",
  "description": "Showichiroのカスタムプラグインマーケットプレイス",
  "owner": {
    "name": "Showichiro"
  },
  "plugins": [
    {
      "name": "obsidian-to-notion",
      "source": "./plugins/obsidian-to-notion",
      "description": "ObsidianのドキュメントをNotionに反映するプラグイン",
      "version": "1.0.1",
      "author": {
        "name": "Showichiro"
      },
      "keywords": ["obsidian", "notion", "sync", "markdown", "document"]
    },
    {
      "name": "worktree-plus",
      "source": "./plugins/worktree-plus",
      "description": "Git worktree管理を支援するプラグイン（wtp連携）",
      "version": "1.0.0",
      "author": {
        "name": "Showichiro"
      },
      "keywords": ["git", "worktree", "wtp", "branch", "workspace"]
    },
    {
      "name": "openai-codex",
      "source": "./plugins/openai-codex",
      "description": "OpenAI Codex CLI（非対話モード）を呼び出すプラグイン",
      "version": "1.0.0",
      "author": {
        "name": "Showichiro"
      },
      "keywords": ["openai", "codex", "gpt", "coding-agent"]
    },
    {
      "name": "gog-cli",
      "source": "./plugins/gog-cli",
      "description": "Google Suite CLI (gog) の操作を支援するプラグイン — Gmail, Calendar, Drive, Contacts, Tasks, Sheets, Docs, Slides, Chat, Classroom, People, Groups, Keep",
      "version": "2.0.0",
      "author": {
        "name": "Showichiro"
      },
      "keywords": ["google", "gmail", "calendar", "drive", "gog", "cli", "sheets", "tasks", "contacts", "chat", "classroom", "docs", "slides", "people", "groups", "keep"]
    },
    {
      "name": "yap-transcribe",
      "source": "./plugins/yap-transcribe",
      "description": "音声・動画ファイルの文字起こしを支援するプラグイン（yap連携）",
      "version": "1.0.0",
      "author": {
        "name": "Showichiro"
      },
      "keywords": ["yap", "transcribe", "speech", "audio", "video", "srt", "caption", "文字起こし"]
    },
    {
      "name": "obsidian-cli",
      "source": "./plugins/obsidian-cli",
      "description": "Obsidian CLIを使ったノート操作を支援するプラグイン",
      "version": "1.0.0",
      "author": {
        "name": "Showichiro"
      },
      "keywords": ["obsidian", "obsidian-cli", "note", "vault", "markdown", "ノート", "ナレッジベース"]
    }
  ]
}
